### AGENTS
%agent: A(s,loc~cytosol,loc_index_1~0~1)
%agent: B(s,loc~cytosol,loc_index_1~0~1)

### INITIAL VALUES
%init: 1000 A(s!1,loc~cytosol,loc_index_1~0),B(s!1,loc~cytosol,loc_index_1~1)

### VARIABLES
%var: 'all monomer A' A(s)
%var: 'all monomer B' B(s)
%var: 'all dimer' A(s!_)
%var: 'dimer AB' A(s!1,loc~cytosol,loc_index_1~0),B(s!1,loc~cytosol,loc_index_1~1)
%var: 'dimer BA' A(s!1,loc~cytosol,loc_index_1~1),B(s!1,loc~cytosol,loc_index_1~0)
%var: 'same voxel dimer AB' A(s!1,loc~cytosol),B(s!1,loc~cytosol)
%var: 'same voxel dimer BA' A(s!1,loc~cytosol),B(s!1,loc~cytosol)

### PLOTS
%plot: 'all monomer A'
%plot: 'all monomer B'
%plot: 'all dimer'
%plot: 'dimer AB'
%plot: 'dimer BA'
%plot: 'same voxel dimer AB'
%plot: 'same voxel dimer BA'

